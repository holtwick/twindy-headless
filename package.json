{
  "name": "twindy-headless",
  "version": "0.9.2",
  "private": false,
  "types": "./types/index.d.ts",
  "main": "./dist/twindy-headless.umd.js",
  "module": "./dist/twindy-headless.es.js",
  "exports": {
    ".": {
      "import": "./dist/twindy-headless.es.js",
      "require": "./dist/twindy-headless.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vuedx-typecheck . && vite build",
    "check": "vuedx-typecheck .",
    "serve": "vite preview",
    "format": "prettier --write .",
    "prepublishOnly": "npm run format && npm run build"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.8.3",
    "@vueuse/core": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.6",
    "@vuedx/typecheck": "^0.6.3",
    "@vuedx/typescript-plugin-vue": "^0.6.3",
    "prettier": "^2.2.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.2.2",
    "vite": "^2.0.3"
  }
}
